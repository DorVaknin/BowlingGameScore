  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test TEST_MONGODB_URI=mongodb://127.0.0.1:27017/BowlingScoresDB node --inspect ./node_modules/.bin/mocha 'tests/**/*.js'"
  },
  "keywords": [],
  "author": "",
