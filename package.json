    "express": "^4.18.2",
    "mongoose": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3"
  }
